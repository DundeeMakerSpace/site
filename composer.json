{
    "name": "dundeemakerspace/dundeemakerspace-site",
    "description": "The Dundee MakerSpace WordPress theme and custom plugins",
    "authors": [
        {
            "name": "Grant Richmond",
            "email": "mail@grant.codes"
        }
    ],
    "config": {
    	"vendor-dir": "mu-plugins/terminally-pixelated/vendor"
    },
    "extra": {
        "installer-paths": {
            "mu-plugins/terminally-pixelated/vendor/{$name}/": ["htmlburger/carbon-fields"],
            "mu-plugins/terminally-pixelated/vendor/{$name}/": ["webdevstudios/cmb2"]
        }
    },
    "autoload": {
        "files": [
            "mu-plugins/terminally-pixelated/vendor/zamoose/themehookalliance/tha-theme-hooks.php",
            "mu-plugins/terminally-pixelated/vendor/webdevstudios/cmb2/init.php"
        ]
    },
    "minimum-stability": "dev",
    "require": {
        "tgmpa/tgm-plugin-activation": "^2.5",
        "zamoose/themehookalliance": "dev-master",
        "webdevstudios/cmb2": "v2.2.2.1",
        "endroid/qrcode": "^1.7",
        "htmlburger/carbon-fields": "^1.5"
    }
}
